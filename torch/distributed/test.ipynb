        "import torch.nn as nn\n",
        "class ToyModel(nn.Module):\n",
        "    def __init__(self):\n",
        "        super(ToyModel, self).__init__()\n",
        "        self.net1 = nn.Linear(16, 16)\n",
        "        self.relu = nn.ReLU()\n",
        "        self.net2 = nn.Linear(16, 8)\n",
        "\n",
        "    def forward(self, x):\n",
        "        return self.net2(self.relu(self.net1(x)))\n",
        "model = ToyModel()"
      ]
    },
